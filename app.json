{
  "expo": {
    "name": "Trails & Trial",
    "slug": "loc",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package":"com.Khushboo.TrailsTrial",
      "permissions":["ACCESS_FINE_LOCATION"],
      "config":{
        "googleMaps":{
          "apiKey": "yourKey"
        }
      }
    }
  }
}
